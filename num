import java.util.*;
class Num
{
 public static void main(String args[])
{
  System.out.println("Enter a number");
   Scanner sc= new Scanner(System.in);
   int x= sc.nextInt();
   if(x%2==0)
    { 
      System.out.println("number is even");
    }
   else
{
    System.out.print("number is odd");
  }
}
}
